<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <welcome-file-list>
    <welcome-file>PIndexServlet</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>PRegisterServlet</display-name>
    <servlet-name>PRegisterServlet</servlet-name>
    <servlet-class>com.cinema.prosenium.servlet.PLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PRegisterServlet</servlet-name>
    <url-pattern>/PRegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PTicketServlet</display-name>
    <servlet-name>PTicketServlet</servlet-name>
    <servlet-class>com.cinema.prosenium.servlet.PScheduleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PTicketServlet</servlet-name>
    <url-pattern>/PTicketServlet</url-pattern>
  </servlet-mapping>
</web-app>